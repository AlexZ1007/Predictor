AA,CAAA,GAAA,CAAA,QAAA,EAAA,eAAA;AACA,YAAA,QAAA,GAAA,KAAA;AACA;AACA;;AACA,cAAA,CAAA,CAAA,CAAA,MAAA,EAAA,CAAA,CAAA,GAAA,GAAA,KAAA,CAAA,qBAAA,CAAA,EAAA;AAAA;AACA,YAAA,CAAA,CAAA,MAAA,EAAA,CAAA,CAAA,GAAA,CAAA,EAAA;AACA,YAAA,CAAA,CAAA,MAAA,EAAA,CAAA,CAAA,IAAA,CAAA,aAAA,EAAA,6BAAA;AACA,YAAA,CAAA,CAAA,MAAA,EAAA,CAAA,CAAA,GAAA,CAAA,QAAA,EAAA,kBAAA;AACA,YAAA,QAAA,GAAA,KAAA;AACA;AACA,WAdA,CAeA;;;AACA,eAAA,OAAA,CAAA,MAAA,CAAA,CAAA,EAAA,IAAA,CAAA;AAAA,YAAA,IAAA,EAAA,CAAA,CAAA,MAAA,EAAA,CAAA,CAAA,GAAA,EAAA;AAAA,YAAA,MAAA,EAAA,CAAA;AAAA,YAAA,KAAA,EAAA,CAAA;AAAA,YAAA,EAAA,EAAA;AAAA,WAAA;AACA,SAnBA,CAoBA;;;AACA,QAAA,YAAA,CAAA,MAAA,GAAA,IAAA,CAAA,SAAA,CAAA,KAAA,OAAA,CAAA,MAAA,CAAA;AACA;;AACA,UAAA,CAAA,QAAA,EAAA,OAAA,CAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,4BAAA,KAAA,MAAA;AACA,KA7BA;AA+BA,IAAA,QA/BA,oBA+BA,EA/BA,EA+BA;AACA,MAAA,CAAA,CAAA,MAAA,EAAA,CAAA,CAAA,GAAA,CAAA,QAAA,EAAA,iBAAA;AACA,MAAA,CAAA,CAAA,MAAA,EAAA,CAAA,CAAA,IAAA,CAAA,aAAA,EAAA,kBAAA;AACA,KAlCA;AAoCA,IAAA,oBApCA,kCAoCA;AACA,UAAA,MAAA,CAAA,UAAA,GAAA,GAAA,EAAA;AAAA,QAAA,CAAA,CAAA,cAAA,CAAA,CAAA,WAAA,CAAA,YAAA;AAAA,QAAA,CAAA,CAAA,cAAA,CAAA,CAAA,QAAA,CAAA,YAAA;AAAA,OAAA,MACA,IAAA,MAAA,CAAA,UAAA,GAAA,IAAA,EAAA;AAAA,QAAA,CAAA,CAAA,cAAA,CAAA,CAAA,WAAA,CAAA,YAAA;AAAA,QAAA,CAAA,CAAA,cAAA,CAAA,CAAA,QAAA,CAAA,YAAA;AAAA,OAAA,MACA;AAAA,QAAA,CAAA,CAAA,cAAA,CAAA,CAAA,QAAA,CAAA,YAAA;AAAA,QAAA,CAAA,CAAA,cAAA,CAAA,CAAA,WAAA,CAAA,YAAA;AAAA,QAAA,CAAA,CAAA,cAAA,CAAA,CAAA,WAAA,CAAA,YAAA;AAAA;AACA;AAxCA;AAfA,CAAA","sourcesContent":["<template>\r\n  <div class=\"groups container\">\r\n    <div class=\"controls d-flex justify-content-between\">\r\n      <h3>Insert the groups</h3>\r\n      <router-link class=\"btn btn-dark btn-color-pink btn-hover-aqua\" to=\"/prediction/groupstage\" event=\"\" @click.native.prevent=\"processGroups()\">Confirm</router-link>\r\n    </div>\r\n    \r\n    <div class=\"row row-cols-3\">\r\n      <div class=\"group col\" v-for=\"i in groups\" :key=\"i\">\r\n        <div class=\"card-header\">\r\n          GROUP #{{i}} \r\n        </div>\r\n        <ul class=\"list-group list-group-flush\">\r\n          <li class=\"list-group-item\" v-for=\"n in teamsPerGroup\" :key=\"n\">\r\n            <input  type=\"text\" class=\"form-control\" @change=\"resetCSS(i.toString()+n.toString())\" \r\n                    placeholder=\"Insert team name\" :id=\"i.toString()+n.toString()\" autocomplete=\"off\">\r\n          </li>\r\n        </ul>\r\n      </div>\r\n    </div>     \r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport $ from 'jquery'\r\nexport default {\r\n    created() {\r\n      window.addEventListener(\"resize\", this.makeDesignResponsive);\r\n    },\r\n    destroyed() {\r\n      window.removeEventListener(\"resize\", this.makeDesignResponsive);\r\n    },\r\n    data: function(){\r\n      return{\r\n        groups: this.$parent.competions[this.compID].groups,\r\n        teamsPerGroup: this.$parent.competions[this.compID].teamsPerGroup,\r\n      }\r\n    },\r\n    mounted(){this.makeDesignResponsive()},\r\n    props: ['compID'],\r\n    methods:{\r\n      processGroups(){\r\n          this.$parent.groups=[[]] // Reset the groups \r\n          let redirect=true;\r\n          for (let i=1; i<=this.groups; i++){ \r\n            this.$parent.groups.push([{}]);\r\n            for(let n=1; n<=this.teamsPerGroup; n++){\r\n              let id=i+''+n\r\n              if(!$(\"#\"+id).val()){  // Check if the input is empty\r\n                $(\"#\"+id).attr(\"placeholder\", \"Input can't be empty!\");\r\n                $(\"#\"+id).css(\"border\",\"1px solid red\");\r\n                redirect=false;\r\n                continue;\r\n              }\r\n              if(!$(\"#\"+id).val().match(/([a-zA-Z])+([ -~])*/)){   //Check if the input hasn t only empty spaces\r\n                $(\"#\"+id).val(\"\");\r\n                $(\"#\"+id).attr(\"placeholder\", \"Insert at least one letter!\");\r\n                $(\"#\"+id).css(\"border\",\"1px solid orange\");\r\n                redirect=false;\r\n                continue;\r\n              }\r\n              // Insert the teams into the groups variable  \r\n              this.$parent.groups[i].push({name: $(\"#\"+id).val(), points: 0, place: 0, GD: 0})\r\n            }\r\n            // Save the groups into local storage\r\n            localStorage.groups=JSON.stringify(this.$parent.groups);\r\n          }\r\n          if(!redirect) return 0;\r\n          this.$router.push(\"/prediction/groupStage/\"+this.compID);\r\n      },\r\n\r\n      resetCSS(id){\r\n          $(\"#\"+id).css(\"border\",\"1px solid black\");\r\n          $(\"#\"+id).attr(\"placeholder\",\"Insert team name\");\r\n      },\r\n\r\n      makeDesignResponsive(){\r\n        if(window.innerWidth<790){ $(\".groups .row\").removeClass(\"row-cols-2\"); $(\".groups .row\").addClass(\"row-cols-1\");  }\r\n        else if(window.innerWidth<1000){ $(\".groups .row\").removeClass(\"row-cols-3\"); $(\".groups .row\").addClass(\"row-cols-2\");} \r\n        else{  $(\".groups .row\").addClass(\"row-cols-3\"); $(\".groups .row\").removeClass(\"row-cols-2\"); $(\".groups .row\").removeClass(\"row-cols-1\");}\r\n      }\r\n    }\r\n    \r\n}\r\n</script>"],"sourceRoot":"src/components"}]}